{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": [
      "node"
    ],
    "paths": {
      "@Components/*": [
          "components/*"
      ],
      "@Common/*": [
          "components/common/*"
      ],
      "@Assets/*": [
          "assets/*"
      ],
      "@Configs/*": [
          "configs/*"
      ],
      "@Pages/*": [
          "pages/*"
      ],
      "@Layouts/*": [
          "layouts/*"
      ],
      "@Themes/*": [
          "themes/*"
      ],
      "@StyleSheets/*": [
          "stylesheets/*"
      ],
      "@Interfaces/*": [
          "interfaces/*"
      ],
      "@Metas/*": [
          "metas/*"
      ],
      "@Helpers/*": [
          "helpers/*"
      ],
      "@Utils/*": [
          "utils/*"
      ],
      "@Hooks/*": [
          "hooks/*"
      ],
      "@Hocs/*": [
          "hocs/*"
      ],
      "@Reduxs/*": [
          "reduxs/*"
      ],
      "@Recoils/*": [
          "recoils/*"
      ],
      "@Apis/*": [
          "apis/*"
      ],
      "@Constants/*": [
          "constants/*"
      ]
  }
  },
  "include": ["./src", "env"]
}
